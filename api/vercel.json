{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node@20.x",
      "config": {
        "includeFiles": [
          "src/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ]
}